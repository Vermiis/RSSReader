<Window x:Class="RSSReader.ReadRSSWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSSReader"
        mc:Ignorable="d"
        Title="ReadRSSWindow" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="123*"/>
            <RowDefinition Height="146*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btn_Close" Content="Zamknij" Margin="0,0,10,10" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Center" Click="btn_Close_Click" Grid.Row="1" HorizontalAlignment="Right" Width="75" Height="20" VerticalAlignment="Bottom"/>
        <WebBrowser x:Name="Wb_Article" Margin="10,10,10,35" Grid.RowSpan="2"/>

    </Grid>
</Window>
